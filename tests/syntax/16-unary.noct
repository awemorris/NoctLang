func main() {
    var a = 1;
    if (!a) {
        print("BAD");
    } else {
        print("OK");
    }

    a = 0;
    if (!a) {
        print("OK");
    } else {
        print("BAD");
    }

    a = 123;
    var b = -a;
    if (b == -123) {
        print("OK");
    } else {
        print("BAD");
    }
}
