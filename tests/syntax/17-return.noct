func main() {
    if (foo()) {
        print("True");
    } else {
        print("False");
    }

    if (bar()) {
        print("True");
    } else {
        print("False");
    }

    no_value();
}

func foo() {
    var a = 0;
    if (a == 0) {
        return 1;
    } else if (a == 1) {
        return 0;
    } else {
        return 0;
    }
    print("BAD");
    return 0;
}

func bar() {
    var a = 1;
    if (a == 0) {
        return 0;
    } else if (a == 1) {
        return 1;
    } else {
        return 0;
    }
    print("BAD");
    return 0;
}

func no_value() {
     return;
}
